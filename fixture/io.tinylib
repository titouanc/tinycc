int _printInt(int n){
    if (n == 0){
        print('0');
    } else {
        if (n >= 10){
            _printInt(n/10);
        }
        print('0' + n%10);
    }
    return 1;
}

int printInt(int n){
    if (n < 0){
        print('-');
        _printInt(0-n);
    } else {
        _printInt(n);
    }
    print('\n');
    return 1;
}

int readInt(){
    int r;
    r = 0;
    char c;
    c = read();
    while (c != '\n'){
        r *= 10;
        r += (c - '0');
    }
    return r;
}
